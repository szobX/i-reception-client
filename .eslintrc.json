{
  "extends": ["plugin:vue/recommended"],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": ["eslint-plugin-vue", "eslint-plugin-import"],
  "env": {
    "es6": false
  }
}
